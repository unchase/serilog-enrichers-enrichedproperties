<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Unchase</Authors>
    <Description>Enriches Serilog events with information from logger properties that was enriched earlier.</Description>
    <Copyright>Copyright 2018 Unchase</Copyright>
    <PackageLicenseUrl>https://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/unchase/serilog-enrichers-enrichedproperties</PackageProjectUrl>
    <PackageTags>enricher;C#;.net Core;serilog;logging;semantic;structured</PackageTags>
    <RepositoryUrl>https://github.com/unchase/serilog-enrichers-enrichedproperties</RepositoryUrl>
    <NeutralLanguage>en</NeutralLanguage>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <RepositoryType>Github</RepositoryType>
    <Version>1.0.0</Version>
    <DocumentationMarkdownURL>assets/README.md</DocumentationMarkdownURL>
    <PackageIconUrl>https://serilog.net/images/serilog-nuget.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Serilog" Version="2.7.1" />
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
  </ItemGroup>

</Project>
